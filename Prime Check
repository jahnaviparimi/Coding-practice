#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    if (N <= 1) {
        printf("NO");
        return 0;
    }

    int i = 2;
    while (i * i <= N) {
        if (N % i == 0) {
            printf("NO");
            return 0;
        }
        i++;
    }

    printf("YES");
    return 0;
}
